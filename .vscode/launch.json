{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch myapp1",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "myapp1", "--inspect"],
      "console": "integratedTerminal",
      "sourceMapPathOverrides": {
        "webpack:///*": "${workspaceRoot}/apps/middleware/*"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch my-nest-app",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "my-nest-app", "--inspect"],
      "console": "integratedTerminal",
      "sourceMapPathOverrides": {
        "webpack:///*": "${workspaceRoot}/apps/middleware/*"
      }
    }
  ]
}
