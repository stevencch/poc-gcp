{
  "name": "@poc-gcp1/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "docker": "docker run -it --rm -p 4112:4112 --env-file ./.env my-nest-app",
    "docker:build": "npm run build && docker build -t my-nest-app:latest -f ./apps/my-nest-app/Dockerfile .",
    "build:errorHandler": "nx run errorHandler:build",
    "build": "nx affected --target=build",
    "test": "nx affected --target=test --exclude=*-e2e",
    "test:e2e": "nx affected --target=e2e",
    "format": "nx format:check",
    "format:fix": "nx format:write",
    "lint": "nx affected --target=lint",
    "start": "nx run-many -t serve -p my-nest-app myapp1 --parallel",
    "mocks": "mocks-server",
    "generate:sql": "bun packages/database/src/scripts/generate-sql.ts",
    "migrate:latest": "knex migrate:latest --knexfile=packages/database/src/lib/knex/knexfile.ts --env=local",
    "migrate:revert": "knex migrate:rollback --knexfile=packages/database/src/lib/knex/knexfile.ts --env=local",
    "migrate:rollback": "knex migrate:rollback --all --knexfile=packages/database/src/lib/knex/knexfile.ts --env=local",
    "seed:run": "knex seed:run --knexfile=packages/database/src/lib/knex/knexfile.ts --env=local",
    "setup": "docker compose run -it --rm emulator-setup",
    "docker:dev_env": "docker compose --profile dev_env -f docker-compose.yml up -d --build",
    "docker:dev_env:down": "docker compose --profile dev_env -f docker-compose.yml down"
  },
  "private": true,
  "dependencies": {
    "@commercetools/platform-sdk": "^7.17.0",
    "@commercetools/sdk-client-v2": "^2.5.0",
    "@google-cloud/bigquery": "^7.9.1",
    "@google-cloud/cloud-sql-connector": "^1.4.0",
    "@google-cloud/firestore": "^7.10.0",
    "@google-cloud/functions-framework": "^3.4.2",
    "@google-cloud/pubsub": "^4.8.0",
    "@google-cloud/storage": "^7.14.0",
    "@google-cloud/tasks": "^5.5.1",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.2",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.0.2",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudevents": "^8.0.2",
    "csv-writer": "^1.6.0",
    "http-proxy-middleware": "^3.0.3",
    "ioredis": "^5.4.2",
    "knex": "^3.1.0",
    "mssql": "^11.0.1",
    "nestjs-pino": "^4.2.0",
    "newrelic": "^12.9.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.13.1",
    "pino-http": "^10.3.0",
    "protobufjs": "^7.4.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "save": "^2.9.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@mocks-server/main": "^4.1.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/esbuild": "20.0.3",
    "@nx/eslint": "20.0.3",
    "@nx/eslint-plugin": "20.0.3",
    "@nx/jest": "20.0.3",
    "@nx/js": "20.0.3",
    "@nx/nest": "^20.0.3",
    "@nx/node": "20.0.3",
    "@nx/web": "20.0.3",
    "@nx/webpack": "20.0.3",
    "@nx/workspace": "20.0.3",
    "@pact-foundation/pact": "^13.2.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.12",
    "@types/mssql": "^9.1.5",
    "@types/node": "~18.16.9",
    "@types/node-fetch": "^2.6.12",
    "bun": "^1.1.33",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lefthook": "^1.8.2",
    "nock": "^14.0.0-beta.15",
    "nx": "20.0.3",
    "prettier": "^2.6.2",
    "sql-formatter": "^15.4.5",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.5.2",
    "typescript-eslint": "^8.0.0",
    "webpack-cli": "^5.1.4"
  }
}
